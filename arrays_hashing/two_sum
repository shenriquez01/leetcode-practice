class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        values_counted = {}

        for i in range(len(nums)):
            diff = target - nums[i]
            if diff in values_counted:
                first_index = values_counted.get(diff)
                return [first_index, i]
            else:
                values_counted[nums[i]] = i
